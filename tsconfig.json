{
    "compilerOptions": {
        "lib": [
            "ESNext",
            "dom"
        ],
        "outDir": "lib",
        "removeComments": true,
        "target": "ES6",
        "baseUrl": "./",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "paths": {},
        "sourceMap": true,
        "alwaysStrict": true,
        "allowUnreachableCode": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    },
    "include": ["./**/*.ts"],
    "exclude": []
}
